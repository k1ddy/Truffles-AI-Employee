# OpenAI
OPENAI_API_KEY=
FAST_MODEL=gpt-5-mini
SLOW_MODEL=gpt-5-mini
FAST_MODEL_MAX_CHARS=160
INTENT_TIMEOUT_SECONDS=1.5
LLM_TIMEOUT_SECONDS=4
LLM_MAX_TOKENS=600
LLM_HISTORY_MESSAGES=6
LLM_KNOWLEDGE_CHARS=1500
LLM_CACHE_ENABLED=1
LLM_CACHE_TTL_SECONDS=86400
LLM_CACHE_SOCKET_TIMEOUT_SECONDS=0.3
POLICY_VERSION=v1

# Audio transcription (ASR)
AUDIO_TRANSCRIPTION_ENABLED=0
AUDIO_TRANSCRIPTION_MAX_MB=2
AUDIO_TRANSCRIPTION_MODEL=whisper-1
AUDIO_TRANSCRIPTION_LANGUAGE=
ASR_PRIMARY_PROVIDER=elevenlabs
ASR_FALLBACK_PROVIDER=openai_whisper
ASR_ELEVENLABS_MODEL_ID=scribe_v1
ASR_TIMEOUT_SECONDS=6
ASR_MIN_CHARS=12
ELEVENLABS_API_KEY=

# Outbox
OUTBOX_WORKER_ENABLED=1
OUTBOX_WORKER_INTERVAL_SECONDS=1
OUTBOX_COALESCE_SECONDS=1
OUTBOX_WINDOW_MERGE_SECONDS=2.5

# Qdrant
QDRANT_HOST=http://qdrant:6333
QDRANT_API_KEY=

# BGE-M3 Embedding Service
BGE_M3_URL=http://bge-m3:80/embed

# Database
DATABASE_URL=postgresql://user:password@postgres:5432/chatbot
